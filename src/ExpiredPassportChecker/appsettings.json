{
  "MainSettings": {
    "SourceBaseUrl": "https://проверки.гувм.мвд.рф/upload/expired-passports/",
    "SourceFileName": "list_of_expired_passports.csv.bz2",
    "UnpackBufferSize": 4096,
    "CronSchedule": "* * * * *",
    "EnabledSteps": {
      "DownloadBzip2": true,
      "UnpackFromBzip2": true,
      "PackCsvToPassportData": true,
      "RepackBzip2ToPassportData": false,
      "DeleteBzip2AfterUnpack": true,
      "DeleteCsvAfterProcessing": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
